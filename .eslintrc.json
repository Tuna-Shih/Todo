{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "extends": [
    "fbjs/strict",
    "google",
    "plugin:prettier/recommended",
    "prettier/standard"
  ],
  "require-jsdoc": [
    "warn",
    {
      "require": {
        "FunctionDeclaration": true,
        "ClassDeclaration": false,
        "MethodDefinition": false,
        "ArrowFunctionExpression": false
      }
    }
  ]
}
